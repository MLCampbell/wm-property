  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "buffer": "^6.0.3",
    "parcel": "^2.7.0",
    "postcss": "^8.4.18",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "simple-webp-converter": "^1.0.3",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {
    "cloudinary": "^1.32.0",
    "postcss-nested": "^6.0.0"
  }
}
